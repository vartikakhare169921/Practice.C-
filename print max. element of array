//to find maximum element of an array
#include <iostream>
using namespace std;
int main() {
int n,max=-873474;
cout<<"enter size of array: ";
cin>>n;
cout<<"enter the elements of array: ";
int A[n];
for(int i=0;i<5;i++)
{
    cin>>A[i];
    if(A[i]>max)
    {
    max=A[i];
    }
}
cout<<max;
    return 0;
}
