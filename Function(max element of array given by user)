#include <iostream>
using namespace std;
int n;
int getmax(int A[])
{
    int max=-8574646;
    for(int i=0;i<n;i++)
    {
        if(A[i]>max)
        max=A[i];
    }
    return max;
}
int main()
{
    cout<<"enter size of array: ";
    cin>>n;
    int A[n];
    cout<<"enter the elements of array: ";
    for(int i=0;i<n;i++)
    {
        cin>>A[i];
    }
    cout<<getmax(A);
    return 0;
}
