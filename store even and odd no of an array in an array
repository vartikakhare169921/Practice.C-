#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"enter the size of array ";
    cin>>n;
    cout<<"enter the elements of array ";
    int A[n];
    for(int i=0;i<n;i++)
    {
        cin>>A[i];
    }
    int odd[n],even[n],k=0,l=0;
    for(int i=0;i<n;i++)
    {
        if(A[i]%2==0)
        {
            even[k]=A[i];
            k++;
        }
        else
        {
        odd[l]=A[i];
        l++;
        }
    }
        for(int i=0;i<k;i++)
        {
            cout<<even[i]<<" ";
        }
        cout<<endl;
         for(int i=0;i<l;i++)
        {
            cout<<odd[i]<<" ";
        }
        
    return 0;
}
