#include <iostream>
using namespace std;
int main()
{
    int n,Result;
    cin>>n;
    int A[n];
    for(int i=0;i<n;i++)
    {
        cin>>A[i];
    }
    int max=A[0];
    int min=A[0];
    for(int i=0;i<n;i++)
    {
       if(A[i]>max)
       {
           max=A[i];
       }
    }
    for(int i=0;i<n;i++)
    {
       if(A[i]<min)
       {
           min=A[i];
       }
    }
    Result=max-min;
    cout<<"Result = "<<Result;
    
    return 0;
}
