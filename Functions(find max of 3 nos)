//to find maximum of 3 given nos
#include <iostream>
using namespace std;
int max(int x,int y,int z)
{
    if(x>y && x>z)
    return x;
    else if(y>z)
    return y;
    else 
    return z;
}
int main()
{
    int a=5,b=6,c=7,result;
   
    result = max(a,b,c);
    cout<<"maximum no. is "<<result;
    return 0;
}
