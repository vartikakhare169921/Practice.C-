#include<iostream>
using namespace std;
int main()
{
    int t,n,k,c=0;
    cin>>t;
    while(t--)
    {
        cin>>n>>k;
        int A[n];
        for(int i=0;i<n;i++)
        {
            cin>>A[i];
        }
        for(int i=0;i<n;i++)
        {
            if(A[i]>=0)
            {
                c++;
            }
        }
        if(c>=k)
        {
            cout<<"NO"<<endl;
        }       
        else
        {
           cout<<"YES"<<endl;
        } 
    }
    return 0;
}
