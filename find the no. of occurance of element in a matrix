#include <iostream>
using namespace std;
int main()
{
    int m,n,x,c=0;
    cout<<"enter the size ";
    cin>>m>>n;
    int A[m][n];
    cout<<"enter the elements of matrix ";
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
            cin>>A[i][j];
        }
    }
    cout<<"enter the element ";
    cin>>x;
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
          if(A[i][j]==x)
          c++;
        }
    }
    cout<<c;
    return 0;
}
