#include <iostream>
using namespace std;
int main() {
    for(int i=0;i<4;i++)
    {
        for(int j=0;j<4;j++)
        {
            if(j==0)
            cout<<"4";
            else if(j==1 && i!=0)
            cout<<"3";
            else if(j==2 && i!=0 && i!=1)
            cout<<"2";
            else if(j==3 && i==3)
            cout<<"1";
        }
        cout<<endl;
    }

    return 0;
}
