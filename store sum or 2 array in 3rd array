#include <iostream>
using namespace std;
int main()
{
int m;
cout<<"enter the size of arrays ";
cin>>m;
cout<<"enter the elements of 1st array ";
int A[m],B[m],C[m];
for(int i=0;i<m;i++)
{
    cin>>A[i];
}
cout<<endl;
cout<<"enter the elements of 2nd array ";
for(int i=0;i<m;i++)
{
    cin>>B[i];
}
cout<<endl;
for(int i=0;i<m;i++)
{
    C[i]=A[i]+B[i];
    cout<<C[i]<<" ";
}
cout<<endl;
return 0;
}
