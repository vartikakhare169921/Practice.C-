#include <iostream>
using namespace std;
int gcd(int a,int b)
{
    while(a!=b)
    {
        if(a>b)
        {
            a=a-b;
        }
        else
        b=b-a;
    }
    return a;
}
int main() {
int m,n;
cout<<"enter 2 nos ";
cin>>m>>n;
cout<<"GCD of 2 nos is "<<gcd(m,n);
      return 0;
}
