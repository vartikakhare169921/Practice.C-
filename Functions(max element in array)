#include <iostream>
using namespace std;
int GetMax(int A[])
{    
    int max=-87952;
    for(int i=0;i<5;i++)
    {
        if(A[i]>max)
        {
            max=A[i];
        }
    }
    return max;
}
int main()
{
    int A[5]={1,2,3,4,5};
    int result=GetMax(A);
    cout<<result;
    return 0;
}
