#include <iostream>
using namespace std;
int main() {
int m1,m2,m3,sum,sum1;
cout<<"enter marks of physics: ";
cin>>m1;
cout<<"enter marks of chemistry: ";
cin>>m2;
cout<<"enter marks of maths: ";
cin>>m3;
sum=m1+m2+m3;
sum1=m1+m3;
if(m1>40 && m2>50 && m3>60 && sum1>150)
{
    cout<<"eligible for the course ";
}
else if(sum>200)
{
    cout<<"eligible for the course ";
}
else
cout<<"not eligible ";
  return 0;
}
