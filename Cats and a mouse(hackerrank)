#include <iostream>
#include <cstdlib>
using namespace std;
int main()
{
    int q,a,b,c;
    cin>>q;
    while(q>0)
    {
    cin>>a>>b>>c;
     int d=abs(a-c);
    int e=abs(b-c);
    if(d>e)
    cout<<"Cat B"<<endl;
    else if(e>d)
    cout<<"Cat A"<<endl;
    else
    cout<<"Mouse C"<<endl;  
    q--;
    }
    return 0;
}
