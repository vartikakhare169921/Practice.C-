#include <iostream>
using namespace std;
int main()
{
int m,n;
cout<<"enter the size of matrix ";
cin>>m>>n;
cout<<"enter the elements of matrix ";
int A[m][n];
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
        cin>>A[i][j];
        cout<<A[i][j]<<" ";
    }
    cout<<endl;
}
for(int i=0;i<n;i++)
{
    for(int j=0;j<m;j++)
    {
        cout<<A[j][i]<<" ";
    }
    cout<<endl;
}
    return 0;
}
