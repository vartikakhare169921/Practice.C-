#include <iostream>
using namespace std;
int main()
{
    int m,n;
    cout<<"enter the size ";
    cin>>m>>n;
    int A[m][n];
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
        cin>>A[i][j];
        }
    }
    int T[n][m];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
           T[i][j]=A[j][i];
        }
    }
     for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cout<<T[i][j]<<" ";
        }
        cout<<endl;
    }
    return 0;
}
