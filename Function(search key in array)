#include <iostream>
using namespace std;
 int key;
void fun(int A[5])
{
    for(int i=0;i<5;i++)
    {
        if(A[i]==key)
        {
            cout<<"key found at "<<i;
            exit(0);
        }
    }
    cout<<"key not found";
}
int main()
{
    int A[5]={1,2,3,4,5};
    cout<<"Enter the key: ";
    cin>>key;
    fun(A);
    return 0;
}
