#include <iostream>
using namespace std;
int main()
{
    int i,j,k,c=0;
    cin>>i>>j>>k;
    for(int x=i;x<=j;x++)
    {
       int rev=0;
       int num=x;
       while(num!=0)
       {
       int r=num%10;
       num=num/10;
       rev=rev*10+r;
       }
       if((x-rev)%k==0)
       {
           c++;
       }   
    }
    cout<<c;
    return 0;
}
