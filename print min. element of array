//to find minimum element of an array
#include <iostream>
using namespace std;
int main() {
int n,min=873474;
cout<<"enter size of array: ";
cin>>n;
cout<<"enter the elements of array: ";
int A[n];
for(int i=0;i<5;i++)
{
    cin>>A[i];
    if(A[i]<min)
    {
    min=A[i];
    }
}
cout<<min;
    return 0;
}
